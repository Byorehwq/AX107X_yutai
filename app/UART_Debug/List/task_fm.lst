    229          			fm_ctl.fm_freq = ocx.number;
    230          			fm_rx_set_freq(fm_ctl.fm_freq);
    231          			mem_write_cur_channel();
    232          		}
    233          #endif
    234          
    235          	}
    236          #endif
    237          }
